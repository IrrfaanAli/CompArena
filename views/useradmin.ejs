<!DOCTYPE html>
<!-- Created By CodingNepal - www.codingnepalweb.com -->
<html lang="en" dir="ltr">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!----<title> Website Layout | CodingLab</title>---->
  <link rel="stylesheet" href="/css/new.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.4.1/semantic.min.css" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css"
    integrity="sha512-1sCRPdkRXhBV2PBLUdRb4tMg1w2YPf37qatUFeS7zlBy7jJI8Lf4VHwWfZZfpXtYSLy85pkm9GaYVYMfw5BC1A=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />
  <style>
  </style>
</head>

<body style="background: url('image/bg3.jpg');">
  <div class="main_box">
    <input type="checkbox" id="check">
    <div class="btn_one">
      <label for="check">
        <i class="fas fa-bars"></i>
      </label>
    </div>
    <div class="sidebar_menu">
      <div class="logo">
        <a href="#">CompArena</a>
      </div>
      <div class="btn_two">
        <label for="check">
          <i class="fas fa-times"></i>
        </label>
      </div>
      <div class="menu">
        <ul>
          <li><i class="fas fa-qrcode"></i>
            <a href="/dashboard">Dashboard</a>
          </li>
          <li>
            <i class="fas fa-link"></i>
            <a href="/ua">User mgmt</a>
          </li>
          <li>
            <i class="fas fa-stream"></i>
            <a href="#">Host mgmt</a>
          </li>
          <li>
            <i class="fas fa-calendar-week"></i>
            <a href="/ea">Event mgmt</a>
          </li>

        </ul>
      </div>
      <div class="social_media">
        <ul>
          <a href="#"><i class="fab fa-facebook-f"></i></a>
          <a href="#"><i class="fab fa-twitter"></i></a>
          <a href="#"><i class="fab fa-instagram"></i></a>
          <a href="#"><i class="fab fa-youtube"></i></a>
        </ul>
      </div>
    </div>
    <div>
      <p
        style="margin: 2% 0 0 16%; font-size: 50px;text-shadow: 5px 4px 15px rgba(0, 0, 0, 0.2)  ; color: white; font-weight: bold;">
        User Management</p>
    </div>
    <div
      style="border-radius: 9px;width: 1200px; height: auto; margin: 2% 0 0 16%; background-color: white; box-shadow: 3px 3px 5px 1px rgba(0, 0, 0, 0.2);">
      <table class="ui orange table" style="margin: 50px 0 0 0;">
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Password</th>
          <th>Region</th>
          <th>Interest</th>
          <th>Actions</th>

        </tr>

        <% userList.forEach(function(user) { %>

          <tr>
            <td><%- user.name1 %></td>
            <td><%- user.email1 %></td>
            <td style="color: rgb(84, 81, 81);">********</td>
            <td><%- user.region1 %></td>
            <td><%- user.interest1 %></td>

            <td><a href="/delete_user/<%= user._id %>" <button class='ui yellow button'
                onclick="return confirm('Are you sure you want to delete this item?');">Delete</a>
              <a href="/update_user/<%= user._id %>" <button class='ui orange button'></button>Update</a>
            </td>
          </tr>

          <% }); %>


      </table>
    </div>
  </div>
</body>

</html>